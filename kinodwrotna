function dtheta = kinodwrotna(t,theta)
global J;
global V;
theta1 = theta(1);
theta2 = theta(2);
theta3 = theta(3);
theta4 = theta(4);

 jakobian = subs(J);
 J1=double(jakobian);

   % sterowanie poprzez zadanie wektora prędkości
 dtheta = pinv(J1(1:3,1:4))*[0.005;0:0];
 % stworzenie okręgu ostatnim członem robota
 % dtheta = pinv(J1(1:3,1:4))*[cos(t)/30;sin(t)/30:0];
 end
